#!/bin/zsh

if [ -x /usr/bin/dircolors ]; then
  test -r ~/.dircolors && eval "$(dircolors -b ~/.dircolors)" || eval "$(dircolors -b)"
  alias ls='ls --color=auto'
fi
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'

rude=$(echo gurshpx | tr "[a-z]" "[n-za-m]")
eval $($rude --alias)
eval $($rude --alias oops)

